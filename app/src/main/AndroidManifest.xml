<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triviaapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/t_icon1"
        android:label="Trivia"
        android:roundIcon="@drawable/t_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.activitiess.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.activitiess.OneActivity"></activity>
        <activity android:name=".activities.activitiess.TwoActivity"></activity>
        <activity android:name=".activities.activitiess.SumActivity"></activity>
        <activity android:name=".activities.activitiess.HistoryActivity"></activity>
        <activity android:name=".activities.MainActivity"></activity>

    </application>

</manifest>